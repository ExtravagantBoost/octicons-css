    "prettier": "3.4.1",
    "svg2ttf": "6.0.3",
    "svgo": "3.0.2",
    "svgson": "5.2.1",
    "svgtofont": "6.2.0",
    "trim-newlines": "3.0.1",
    "ttf2woff": "3.0.0",
    "ttf2woff2": "6.0.1",
    "yargs": "15.1.0"
  },
  "eslintConfig": {
